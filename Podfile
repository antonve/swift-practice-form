source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '11.0'
use_frameworks!
inhibit_all_warnings!

plugin 'cocoapods-acknowledgements', :settings_bundle => true, :exclude => ['SwiftLint']

target "swift-practice-form" do
  # Core
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'ReactorKit'

  # UI
  pod 'SnapKit', '~> 3.2.0'

  # Tools
  pod 'SwiftLint'
  pod 'Reveal-SDK', :configurations => ['Debug']
end
